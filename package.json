{
  "name": "double-agent",
  "version": "0.2.8",
  "private": true,
  "description": "Double agent is a detection system for when an http request or browser interaction is not the user agent it says it is",
  "scripts": {
    "build": "tsc --build",
    "watch": "tsc -w",
    "clean": "tsc --build --clean",
    "clean-logger": "tsc --build --clean ./shared/logger"
  },
  "author": "Data Liberation Foundation",
  "license": "ISC",
  "engines": {
    "node": ">=10.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": ". ~/.nvm/nvm.sh && lint-staged"
    }
  },
  "dependencies": {
    "cookie": "^0.4.0",
    "useragent": "^2.3.0",
    "source-map-support": "^0.5.16"
  },
  "devDependencies": {
    "@types/cookie": "^0.3.3",
    "@types/useragent": "^2.1.1",
    "p-queue": "^6.2.1",
    "@types/jest": "^24.0.11",
    "@types/node": "^12.7.11",
    "husky": "^1.3.1",
    "lint-staged": "^8.2.1",
    "nodemon": "^1.18.9",
    "prettier": "^1.19.1",
    "pretty-quick": "^1.8.0",
    "source-map-support": "^0.5.16",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "tslint-plugin-prettier": "^2.0.1",
    "@types/puppeteer": "~1.19.1",
    "typescript": "^3.7.2"
  },
  "workspaces": [
    "detections/*/*",
    "runner"
  ],
  "lint-staged": {
    "linters": {
      "./**/*.ts": [
        "tslint --format verbose",
        "git add"
      ],
      "./**/*.json": [
        "prettier --write",
        "git add"
      ]
    },
    "ignore": [
      "publish/**",
      "**/types/*.js",
      "**/types/*.d.ts",
      "**/results/*.json"
    ]
  }
}
