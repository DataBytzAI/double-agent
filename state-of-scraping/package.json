{
  "name": "state-of-scraping",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "data": "node import/index.js",
    "server": "node server.js",
    "serve": "yarn data && vue-cli-service serve",
    "start": "npm-run-all -l -p server serve",
    "build": "yarn data && vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.2",
    "vue-property-decorator": "^8.3.0",
    "vue-router": "^3.1.6",
    "vue-select": "^3.9.4",
    "vue-slider-component": "^3.1.1",
    "vue-resource": "^1.5.1",
    "vue-string-filter": "^2.0.0",
    "v-tooltip": "^2.0.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.2.3",
    "@vue/cli-plugin-typescript": "^4.2.3",
    "@vue/cli-service": "^4.2.3",
    "lint-staged": "^9.5.0",
    "sass": "^1.25.0",
    "sass-loader": "^8.0.2",
    "typescript": "~3.7.5",
    "vue-template-compiler": "^2.6.11",
    "vue-cli-plugin-pug": "^1.0.7",
    "vue-svg-loader": "^0.16.0",
    "npm-run-all": "^4.1.5"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
